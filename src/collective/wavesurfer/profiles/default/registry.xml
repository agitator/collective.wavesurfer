<?xml version="1.0"?>
<registry>

  <records
      prefix="plone.resources/wavesurfer"
      interface="Products.CMFPlone.interfaces.IResourceRegistry">
    <value key="js">++resource++collective.wavesurfer/wavesurfer.js</value>
  </records>

  <records
      prefix="plone.resources/pat-wavesurfer"
      interface="Products.CMFPlone.interfaces.IResourceRegistry">
    <value key="js">++resource++collective.wavesurfer/pat-wavesurfer.js</value>
  </records>

  <records
      prefix="plone.resources/collective-wavesurfer"
      interface="Products.CMFPlone.interfaces.IResourceRegistry">
    <value key="js">++resource++collective.wavesurfer/bundle.js</value>
    <value key="css">
      <element>++resource++collective.wavesurfer/pat-wavesurfer.less</element>
    </value>
  </records>

  <records
      prefix="plone.bundles/wavesurfer"
      interface="Products.CMFPlone.interfaces.IBundleRegistry">
    <value key="resources">
      <element>collective-wavesurfer</element>
    </value>
    <value key="enabled">True</value>
    <value key="jscompilation">++resource++collective.wavesurfer/wavesurfer-compiled.js</value>
    <value key="csscompilation">++resource++collective.wavesurfer/wavesurfer-compiled.css</value>
    <!-- <value key="last_compilation">2016-12-19 00:00:00</value> -->
    <value key="compile">True</value>
    <value key="depends">plone</value>

    <!-- these stubs are copied over from plone-logged-in and may need updates
         the whole js story isnt perfect her :/
    -->
